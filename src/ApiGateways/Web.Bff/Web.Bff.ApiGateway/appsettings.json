{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    {
      "RouteId": "Identity",
      "ClusterId": "IdentityApi",
      "Match": {
        "Path": "/api/v1/Auth/{everything}"
      }
    },
    {
      "RouteId": "Users",
      "ClusterId": "IdentityApi",
      "Match": {
        "Path": "/api/v1/Users/{everything}"
      }
    },
    {
      "RouteId": "MediaStorage",
      "ClusterId": "MediaStorageApi",
      "Match": {
        "Path": "/api/v1/Image/{everything}"
      }
    }
  ],
  "Clusters": {
    "IdentityApi": {
      "Destinations": {
        "IdentityApi": {
          "Address": "http://identity.api"
        }
      }
    },
    "MediaStorageApi": {
      "Destinations": {
        "MediaStorageApi": {
          "Address": "http://storage.api"
        }
      }
    }
  }
}