{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Auth": {
    "Issuer": "https://identity.api:5001",
    "SigningKey": "secretJWTsigningKey@123"
  },
  "OpenApi": {
    "Endpoint": {
      "Name": "Questions.Aggreagetor V1"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "identityapi.users.route": {
        "ClusterId": "identity.api.cluster",
        "Match": {
          "Path": "/api/v1/Auth/{**catch-all}"
        }
      },
      "identityapi.auth.route": {
        "ClusterId": "identity.api.cluster",
        "Match": {
          "Path": "/api/v1/Users/{**catch-all}"
        }
      },
      "questionsapi.route": {
        "ClusterId": "questionsapi.cluster",
        "Match": {
          "Path": "/api/v1/Questions/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identityapi.cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://identity.api/"
          }
        }
      },
      "questionsapi.cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://questions.api/"
          }
        }
      }
    }
  }
}