{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/Auth/{everything}",
      "UpstreamPathTemplate": "/api/v1/Auth/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "http://identity.api"
        }
      ],
      "SwaggerKey": "Identity"
    },   
    {
      "DownstreamPathTemplate": "/api/v1/Users/{everything}",
      "UpstreamPathTemplate": "/api/v1/Users/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "http://identity.api"
        }
      ],
      "SwaggerKey": "Identity"
    },
    {
      "DownstreamPathTemplate": "/api/v1/Image/{everything}",
      "UpstreamPathTemplate": "/api/v1/Image/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "storage.api"
        }
      ],
      "SwaggerKey": "MediaStorage"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "Identity",
      "Config": [
        {
          "Name": "Identity API",
          "Url": "http://identity.api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "MediaStorage",
      "Config": [
        {
          "Name": "MediaStorage API",
          "Url": "http://storage.api/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway"
  }
}