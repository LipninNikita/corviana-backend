{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "UpstreamPathTemplate": "/api/v1/a/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity.api"
        }
      ],
      "SwaggerKey": "Identity"
    },
    {
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "UpstreamPathTemplate": "/api/v1/s/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "storage.api"
        }
      ],
      "SwaggerKey": "Images"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "Identity",
      "Config": [
        {
          "Name": "Identity API",
          "Url": "http://identity.api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "Images",
      "Config": [
        {
          "Name": "Identity API",
          "Url": "http://storage.api/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway"
  }
}