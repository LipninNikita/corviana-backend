{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "UpstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity.api"
        }
      ],
      "SwaggerKey": "Identity"
    },
    {
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "UpstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "storage.api"
        }
      ],
      "SwaggerKey": "Images"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "Identity",
      "Config": [
        {
          "Name": "Identity API",
          "Url": "http://host.docker.internal:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "Images",
      "Config": [
        {
          "Name": "Identity API",
          "Url": "http://host.docker.internal:5002/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway"
  }
}